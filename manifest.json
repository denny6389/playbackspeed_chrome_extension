{
  "manifest_version": 3,
  "name": "Video Playback Speed Controller",
  "version": "1.0",
  "description": "A Chrome extension to control video playback speed.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}

